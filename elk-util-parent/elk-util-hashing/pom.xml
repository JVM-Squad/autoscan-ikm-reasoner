        <artifactId>elk-util-parent</artifactId>
        <version>0.11.0</version>
    </parent>

    <name>ELK Utilities for Hashing</name>
    <description>ELK Utilities for Hashing</description>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <configuration>
                    <archive>
                        <manifestEntries>
                            <Automatic-Module-Name>dev.ikm.elk.provider.concurrent</Automatic-Module-Name>
                        </manifestEntries>
                    </archive>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
